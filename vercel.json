{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "maxDuration": 30, 
          "memory": 1024  
        }
      }
    ],
    "routes": [
      {
        "src": "/socket.io/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  